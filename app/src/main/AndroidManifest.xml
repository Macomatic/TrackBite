<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.diabeticmealtracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DiabeticMealTracker">
        <activity android:name=".Notifcation_setup_exercise"></activity>
        <activity
            android:name=".Notifcation_Setup_Meal"
            android:label="@string/title_activity_notifcation__setup__meal"
            android:theme="@style/Theme.DiabeticMealTracker.NoActionBar" />
        <activity android:name=".Second_Notification_Page" />
        <activity android:name=".Data_Analysis_Page" />
        <activity android:name=".Notification_Page" />
        <activity android:name=".Exercise_Page" />
        <activity android:name=".Database_Page" />
        <activity android:name=".Input_Page" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>